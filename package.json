    "test": "jest",
    "test:queue": "jest src/queue/EventQueue.test.ts",
    "test:integration": "RUN_INTEGRATION_TESTS=true jest src/__tests__/integration.test.ts --runInBand"
  },
  "dependencies": {
    "ethereum-cryptography": "^3.2.0",
  },
  "devDependencies": {
    "@swc/core": "^1.3.102",
    "@swc/jest": "^0.2.29",
    "@types/jest": "^29.4.0",
    "@types/node": "^25.0.3",
    "jest": "^29.4.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.9.3"
  }
}
